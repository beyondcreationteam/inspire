{
    "name": "beyondcreation/inspire",
    "description": "A Quote will be generated on /inspire",
    "autoload": {
        "psr-4": {
            "Beyondcreation\\Inspire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beyondcreation\\Inspire\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "eyadalasfahany",
            "email": "eelasfahani@beyond-creation.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/sanctum": "^3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beyondcreation\\Inspire\\Providers\\InspirationProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.2",
        "phpunit/phpunit": "^10.0"
    }
}
